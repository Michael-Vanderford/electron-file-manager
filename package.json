{
    "name": "string-file-manager",
    "version": "1.0.2",
    "description": "File Manager",
    "projectUrl": "https://github.com/Michael-Vanderford/electron-file-manager",
    "main": "main.js",
    "license": "GPL-2.0+",
    "maintainer": "Michael Vanderford michael.vanderford@gmail.com",
    "scripts": {
        "start": "electron .",
        "debug": "electron --inspect-brk .",
